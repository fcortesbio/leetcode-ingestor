{
  "name": "leetcode-ingestor",
  "version": "1.0.0",
  "description": "A simple ingestor engine to retrieve and store metadata from LeetCode algorithm problems. This is an individual effort to facilitate my personal learning progress via LeetCode. Metadata is queried from LeetCode via the GraphQL API and stored in SQLite.",
  "keywords": [
    "ingestor",
    "scrapper",
    "leetcode",
    "sqlite",
    "graphql",
    "learning"
  ],
  "homepage": "https://github.com/fcortesbio/leetcode-ingestor#readme",
  "bugs": {
    "url": "https://github.com/fcortesbio/leetcode-ingestor/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fcortesbio/leetcode-ingestor.git"
  },
  "license": "MIT",
  "author": "fcortesbio",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "husky",
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "node --loader ts-node/esm src/index.ts",
    "lint": "eslint .",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,json,css,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "eslint": "^9.39.2",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/sqlite3": "^3.1.11",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
